<% if Pcb.lookup(params[:pcb_name],params[:id]); @pcb=Pcb.lookup(params[:pcb_name],params[:id]); else @pcb=Pcb.all; end %>
<% @pcb_v=Pcb.max_version(params[:pcb_name], params[:id]).first %>
<%= select_tag(:parent_id, options_for_select(@pcb.map{|s|[s.pcb_list, s.id]},if @pcb_v; @pcb_v.id; else nil; end), {:include_blank => true, :class => "select-tag",:id=>"pcb_parent_id"})  %>
